<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
   xmlns:dcterms="http://purl.org/dc/terms/"
   xmlns:owl="http://www.w3.org/2002/07/owl#"
   xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
   xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
   xmlns:skos="http://www.w3.org/2004/02/skos/core#"
>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasCause">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has cause</rdfs:label>
    <rdfs:comment>Links semantic change to its driving cause (external or internal).</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <skos:definition>Relates a semantic change event to the causal factor that drove the change.</skos:definition>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021). External vs internal drivers.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#usesDetectionMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>uses detection method</rdfs:label>
    <rdfs:comment>The computational method used to detect/measure this semantic change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <skos:definition>Specifies the analytical or computational approach employed to identify and characterize a semantic change event.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasDescription">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has description</rdfs:label>
    <rdfs:comment>Human-readable description of the semantic change event.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition>A natural language description explaining the nature, context, and significance of a semantic change event.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#occursInDomain">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>occurs in domain</rdfs:label>
    <rdfs:comment>The disciplinary or discourse domain where semantic change occurs.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition>Specifies the academic discipline, professional field, or discourse community within which a semantic change event manifests.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#WordLevelChange">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Word-Level Change</rdfs:label>
    <rdfs:comment>Aggregate semantic change across all senses of a word.</rdfs:comment>
    <skos:definition>Semantic change measured at the word level, aggregating changes across all senses and usages.</skos:definition>
    <dcterms:bibliographicCitation>Montariol et al. (2021). Scalable and Interpretable Semantic Change Detection. NAACL.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasConfidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has confidence</rdfs:label>
    <rdfs:comment>Researcher's confidence in the semantic change annotation (0.0-1.0).</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <skos:definition>A numerical value between 0.0 and 1.0 indicating the annotator's confidence in the identification and classification of a semantic change event.</skos:definition>
    <dcterms:bibliographicCitation>Hamilton et al. (2016) use statistical significance (p &lt; 0.05) for validation.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasToMeaning">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has to meaning</rdfs:label>
    <rdfs:comment>The new or target meaning after the semantic change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition>Indicates the semantic content or meaning that resulted from a semantic change event.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasEndYear">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has end year</rdfs:label>
    <rdfs:comment>Calendar year when semantic change completed or stabilized (for query convenience).</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition>The calendar year marking the conclusion or stabilization of a semantic change event, cached from temporal interval for efficient querying.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#passesControlTest">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>passes control test</rdfs:label>
    <rdfs:comment>Boolean: whether change observed in genuine but not control condition.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <dcterms:bibliographicCitation>Dubossarsky et al. (2017). Control condition validation.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#InflectionPoint">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Inflection Point</rdfs:label>
    <rdfs:comment>Rapid semantic transition marking shift between distinct meanings. Characterized by abrupt change in term usage patterns.</rdfs:comment>
    <skos:definition>A semantic change event characterized by an abrupt, concentrated shift in meaning occurring within a relatively short temporal window.</skos:definition>
    <skos:example>The term 'agent' in 1995: transition from primarily human actor to computational system in AI discourse.</skos:example>
    <skos:example>The term 'ontology' in 1993: shift from philosophical metaphysics to information science knowledge representation.</skos:example>
    <skos:note>May be operationalized as sudden increase in semantic displacement rate (Hamilton et al. 2016).</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasTemporalScale">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has temporal scale</rdfs:label>
    <rdfs:comment>Scale of temporal span: centuries, decades, years, months.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <skos:definition>The characteristic timescale over which a semantic change event unfolds.</skos:definition>
    <dcterms:bibliographicCitation>Montariol (2021): COVID-19 (months) vs COHA (centuries).</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasControlCondition">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has control condition</rdfs:label>
    <rdfs:comment>Reference to control/null condition for validating genuine vs. artifact change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <dcterms:bibliographicCitation>Dubossarsky et al. (2017, 2019). Control condition requirement.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasTextualEvidence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has textual evidence</rdfs:label>
    <rdfs:comment>Links to specific text segment providing evidence for semantic change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:subPropertyOf rdf:resource="http://ontextract.org/sco#evidencedBy"/>
    <skos:definition>Relates a semantic change event to a specific textual passage that exemplifies or evidences the change.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#DomainNetwork">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Domain Network</rdfs:label>
    <rdfs:comment>Development of discipline-specific semantic network with specialized terminology.</rdfs:comment>
    <skos:definition>The emergence of a domain-specific constellation of meanings and related terms forming a coherent semantic network within a particular field.</skos:definition>
    <skos:example>In AI discourse, 'agent' develops associations with: percept-action loop, rationality, autonomy, multi-agent systems.</skos:example>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#URIDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#StructuralDrift"/>
    <rdfs:label>URI Drift</rdfs:label>
    <rdfs:comment>Changes to concept identifiers/URIs.</rdfs:comment>
    <skos:definition>Modifications to the URI or identifier of an ontology concept, affecting referenceability and version stability.</skos:definition>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). OntoDrift adds URI metric.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasSenseCluster">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has sense cluster</rdfs:label>
    <rdfs:comment>Links change event to specific sense cluster identified via embedding clustering.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SenseLevelChange"/>
    <skos:definition>Associates a sense-level semantic change with the cluster of contextual embeddings representing that sense.</skos:definition>
    <dcterms:bibliographicCitation>Montariol et al. (2021). Cluster-based sense identification.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SubclassDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#StructuralDrift"/>
    <rdfs:label>Subclass Drift</rdfs:label>
    <rdfs:comment>Changes to taxonomic children (specializations below concept).</rdfs:comment>
    <skos:definition>Changes in the subclass relationships of a concept, affecting the taxonomic structure below it in the hierarchy.</skos:definition>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). OntoDrift adds Subclasses metric.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#evidencedBy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>evidenced by</rdfs:label>
    <rdfs:comment>Links semantic change event to documentary evidence supporting its identification.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/BFO_0000016"/>
    <skos:definition>Associates a semantic change event with a document or information artifact that provides evidence for the change.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#Decline">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Decline</rdfs:label>
    <rdfs:comment>Meaning becomes obsolete, archaic, or significantly less frequent in usage.</rdfs:comment>
    <skos:definition>The process by which a particular sense or meaning of a term becomes progressively less common, eventually reaching obsolescence.</skos:definition>
    <skos:example>Decline of 'computer' as human occupation after widespread adoption of electronic computers.</skos:example>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'obsolete words slide into obscurity'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SemanticDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Semantic Drift</rdfs:label>
    <rdfs:comment>Gradual, incremental meaning change over extended period without distinct inflection point.</rdfs:comment>
    <skos:definition>A semantic change process characterized by slow, continuous modification of meaning over an extended temporal interval.</skos:definition>
    <skos:example>The gradual shift in 'computer' from human occupation (1920s-1940s) to electronic machine (1950s onwards).</skos:example>
    <dcterms:bibliographicCitation>Hamilton et al. (2016); Gulla et al. (2010); Stavropoulos et al. (2019).</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasInternalCause">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://ontextract.org/sco#hasCause"/>
    <rdfs:label>has internal cause</rdfs:label>
    <rdfs:comment>Internal linguistic motivations for semantic change.</rdfs:comment>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'only partially understood internal motivations'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SenseLevelChange">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Sense-Level Change</rdfs:label>
    <rdfs:comment>Semantic change in a specific word sense/usage.</rdfs:comment>
    <skos:definition>Semantic change measured at the sense level, tracking evolution of specific word meanings or usages identified through clustering.</skos:definition>
    <dcterms:bibliographicCitation>Montariol et al. (2021). Cluster-based sense-level analysis.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#StructuralDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Structural Drift</rdfs:label>
    <rdfs:comment>Changes to ontology structure: hierarchy, URIs, equivalences.</rdfs:comment>
    <skos:definition>Semantic change affecting the structural aspects of an ontology including URIs, class hierarchies, and equivalence relationships.</skos:definition>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). OntoDrift. MEPDaW@ISWC.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#ExtrinsicDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticDrift"/>
    <rdfs:label>Extrinsic Drift</rdfs:label>
    <rdfs:comment>Concept's semantic value changes relative to real-world phenomena it describes.</rdfs:comment>
    <skos:definition>Semantic drift wherein a concept's meaning shifts with respect to the external reality or phenomena it represents, potentially without internal ontology changes.</skos:definition>
    <skos:note>If whole ontology shows extrinsic but not intrinsic drift, indicates collective drift (domain evolved, ontology internally consistent).</skos:note>
    <dcterms:bibliographicCitation>Gulla et al. (2010). Semantic Drift in Ontologies. WEBIST.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#isSynthetic">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>is synthetic</rdfs:label>
    <rdfs:comment>Boolean: whether this is synthetic (simulated) or authentic (real) semantic change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <dcterms:bibliographicCitation>Dubossarsky et al. (2019). Synthetic task for method validation.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SuperclassDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#StructuralDrift"/>
    <rdfs:label>Superclass Drift</rdfs:label>
    <rdfs:comment>Changes to taxonomic parents (generalizations above concept).</rdfs:comment>
    <skos:definition>Changes in the superclass relationships of a concept, indicating reclassification or generalization shifts in the taxonomy.</skos:definition>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). OntoDrift adds Superclasses metric.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#occursDuringInterval">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>occurs during interval</rdfs:label>
    <rdfs:comment>Links semantic change event to the temporal interval when it occurred.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2006/time#Interval"/>
    <skos:definition>Specifies the temporal region during which a semantic change event took place.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#ClusterBasedMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <rdfs:label>Cluster-Based Method</rdfs:label>
    <rdfs:comment>Clusters contextual embeddings to identify word senses, then tracks sense evolution.</rdfs:comment>
    <dcterms:bibliographicCitation>Montariol et al. (2021). Scalable clustering for sense-level analysis.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#controlledForArtifacts">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>controlled for artifacts</rdfs:label>
    <rdfs:comment>Boolean: whether detection method controlled for frequency/polysemy artifacts.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <dcterms:bibliographicCitation>Dubossarsky et al. (2019). Alignment artifacts.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#StablePolysemy">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Stable Polysemy</rdfs:label>
    <rdfs:comment>Multiple distinct meanings coexist without conflict across disciplines. No single dominant sense emerges.</rdfs:comment>
    <skos:definition>A semantic state wherein a term maintains multiple distinct, stable meanings across different discourse communities without one meaning dominating or replacing others.</skos:definition>
    <skos:example>The term 'agent' in 2024: simultaneously refers to legal person (law), autonomous system (AI), and chemical catalyst (chemistry).</skos:example>
    <dcterms:bibliographicCitation>Hamilton et al. (2016) discuss polysemy as driver of change; this represents stable state without active change.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#EquivalentClassDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#StructuralDrift"/>
    <rdfs:label>Equivalent Class Drift</rdfs:label>
    <rdfs:comment>Changes to owl:equivalentClass relationships.</rdfs:comment>
    <skos:definition>Modifications to equivalence mappings between concepts, affecting ontology alignment and integration.</skos:definition>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). OntoDrift adds Equivalent Classes metric.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasProcessSpecification">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has process specification</rdfs:label>
    <rdfs:comment>Prospective provenance: HOW change should be detected.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Plan"/>
    <dcterms:bibliographicCitation>Missier et al. (2013). D-PROV prospective provenance.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#LinguisticDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticDrift"/>
    <rdfs:label>Linguistic Drift</rdfs:label>
    <rdfs:comment>Slow, regular changes in core meaning driven by internal linguistic factors.</rdfs:comment>
    <skos:definition>Semantic drift resulting from language-internal motivations and systematic patterns of meaning evolution.</skos:definition>
    <dcterms:bibliographicCitation>Kutuzov et al. (2018). Diachronic word embeddings and semantic shifts: a survey. COLING.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <rdfs:label>Semantic Change Ontology</rdfs:label>
    <rdfs:comment>Ontology for modeling semantic change phenomena based on comprehensive literature review</rdfs:comment>
    <rdfs:comment>
    ANNOTATION GUIDELINES (Evidence-Based) v2.0

    1. Event Type Selection:

       ORIGINAL TYPES (v1.0):
       - InflectionPoint: Rapid change (&lt; 5 years typically)
       - StablePolysemy: Multiple meanings coexist without conflict
       - DomainNetwork: Term develops field-specific semantic cluster
       - ConceptualBridge: Works that explicitly connect disciplines
       - SemanticDrift: Gradual change (&gt; 10 years)
       - Emergence: New meaning appears
       - Decline: Meaning becomes obsolete

       SENTIMENT TYPES (Jatowt &amp; Duh 2014):
       - Pejoration: Negative attitude development (e.g., "propaganda")
       - Amelioration: Positive attitude development (e.g., "aggressive" in business)

       LINGUISTIC TYPES (Kutuzov 2018):
       - LinguisticDrift: Language-internal, slow core meaning change
       - CulturalShift: External cultural/social associations change

       ONTOLOGY DRIFT (Gulla 2010, Stavropoulos 2019, Capobianco 2020):
       - IntrinsicDrift: Change relative to other ontology concepts
       - ExtrinsicDrift: Change relative to real-world phenomena
       - CollectiveDrift: Whole domain drifts together
       - LabelDrift: rdfs:label changes
       - IntensionalDrift: Property changes
       - ExtensionalDrift: Instance changes
       - StructuralDrift: Hierarchy/URI/equivalence changes

       LEXICAL LIFECYCLE (Tahmasebi 2021):
       - LexicalEmergence: New word coined/borrowed
       - LexicalObsolescence: Word becomes archaic
       - LexicalReplacement: One word supplants another

       GRANULARITY (Montariol 2021):
       - WordLevelChange: Aggregate across all senses
       - SenseLevelChange: Specific usage/sense tracked

    2. Evidence Requirements:
       - Minimum: 2 documents from different time periods
       - Preferred: Explicit discussion of term usage in scholarly work
       - Ideal: Quantitative frequency data + qualitative analysis
       - Control: Test on randomized corpus (Dubossarsky 2017/2019)

    3. Confidence Scores (Hamilton et al. 2016 significance levels):
       - 0.9-1.0: Strong textual evidence, clear temporal boundaries, p &lt; 0.01
       - 0.7-0.9: Good evidence, approximate boundaries, p &lt; 0.05
       - 0.5-0.7: Suggestive evidence, uncertain boundaries
       - &lt; 0.5: Speculative, requires further investigation

    4. Temporal Precision:
       - Use OWL-Time intervals when possible
       - For approximate dating, use decade-level granularity
       - Document uncertainty in description field
       - Specify hasTemporalScale: centuries|decades|years|months

    5. Detection Method Tracking (REQUIRED for reproducibility):
       - usesDetectionMethod: specify method class
       - controlledForArtifacts: true if frequency/polysemy controlled
       - passesControlTest: true if validated against null condition
       - isSynthetic: true if simulated change, false if authentic

    6. Provenance Tracking (D-PROV - Missier 2013):
       - PROSPECTIVE: hasProcessSpecification (how change SHOULD be detected)
       - RETROSPECTIVE: hasExecutionTrace (what WAS actually detected)
       - Enables reproducibility and method comparison

    7. Cause Attribution (Tahmasebi 2021):
       - hasExternalCause: cultural, societal, technological factors
       - hasInternalCause: linguistic motivations
       - Document evidence for causal claims

    8. Academic References:
       - Cite supporting literature in dcterms:bibliographicCitation
       - Include DOI or permanent URL when available
       - Reference specific page numbers for direct quotes
    </rdfs:comment>
    <dcterms:creator>OntExtract Project</dcterms:creator>
    <dcterms:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-11-21</dcterms:created>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-11-22</dcterms:modified>
    <dcterms:description>Models semantic change events with academic backing from:
        - Lexical semantic change (Hamilton 2016, Kutuzov 2018, Dubossarsky 2019, Montariol 2021)
        - Ontology drift (Stavropoulos 2019, Gulla 2010, Capobianco 2020)
        - Provenance tracking (Missier 2013 D-PROV)
        Integrates BFO upper ontology with PROV-O for research annotations.</dcterms:description>
    <owl:versionInfo>2.0.0</owl:versionInfo>
    <dcterms:bibliographicCitation>
        Hamilton et al. (2016). Diachronic Word Embeddings Reveal Statistical Laws. ACL.
        Stavropoulos et al. (2019). SemaDrift. Journal of Web Semantics.
        Dubossarsky et al. (2019). Time-Out: Temporal Referencing. ACL.
        Gulla et al. (2010). Semantic Drift in Ontologies. WEBIST.
        Tahmasebi et al. (2021). Survey of Computational Approaches. Language Science Press.
        </dcterms:bibliographicCitation>
    <owl:imports rdf:resource="http://purl.obolibrary.org/obo/bfo.owl"/>
    <owl:imports rdf:resource="http://www.w3.org/2006/time"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="http://www.w3.org/ns/prov#"/>
    <rdfs:seeAlso rdf:resource="https://github.com/MatLab-Research/OntExtract/docs/semantic-change-annotation-guidelines"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#CulturalShift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Cultural Shift</rdfs:label>
    <rdfs:comment>Culturally determined changes in word associations, not core meaning.</rdfs:comment>
    <skos:definition>Semantic change driven by external cultural, societal, or technological factors affecting a term's associations rather than its core denotation.</skos:definition>
    <skos:example>Iraq/Syria acquiring war-related associations (Kutuzov 2018).</skos:example>
    <dcterms:bibliographicCitation>Kutuzov et al. (2018). Distinguishes cultural shifts from linguistic drifts.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#LexicalEmergence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#Emergence"/>
    <rdfs:label>Lexical Emergence</rdfs:label>
    <rdfs:comment>New word coined or borrowed from other language.</rdfs:comment>
    <skos:definition>The introduction of an entirely new lexical item (word form) into a language through coinage or borrowing.</skos:definition>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'new words are coined or borrowed from other languages'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#Amelioration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Amelioration</rdfs:label>
    <rdfs:comment>Semantic change toward positive sentiment or attitude (elevation).</rdfs:comment>
    <skos:definition>A semantic change event wherein a term acquires increasingly positive connotations, attitudes, or evaluative associations over time.</skos:definition>
    <skos:example>'aggressive' has undergone amelioration, acquiring positive connotations in business/sports contexts (Jatowt &amp; Duh 2014).</skos:example>
    <skos:altLabel xml:lang="en">Elevation</skos:altLabel>
    <dcterms:bibliographicCitation>Jatowt &amp; Duh (2014). Framework for analyzing semantic change. JCDL. Bloomfield (1933) nine classes.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#Emergence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Emergence</rdfs:label>
    <rdfs:comment>New meaning appears in discourse, often through neologism or semantic extension.</rdfs:comment>
    <skos:definition>The introduction of a previously non-existent meaning or sense for a term within a discourse community.</skos:definition>
    <skos:example>Emergence of 'cloud' as distributed computing infrastructure in late 2000s.</skos:example>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'new words are coined or borrowed from other languages'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#LabelDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Label Drift</rdfs:label>
    <rdfs:comment>Changes in the lexical labels (names) of a concept.</rdfs:comment>
    <skos:definition>Semantic change affecting the rdfs:label or naming of a concept, measured via string similarity (Monge-Elkan).</skos:definition>
    <dcterms:bibliographicCitation>Wang et al. (2009, 2011); Stavropoulos et al. (2019). SemaDrift.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#Pejoration">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Pejoration</rdfs:label>
    <rdfs:comment>Semantic change toward negative sentiment or attitude (degeneration).</rdfs:comment>
    <skos:definition>A semantic change event wherein a term acquires increasingly negative connotations, attitudes, or evaluative associations over time.</skos:definition>
    <skos:example>'fatal' and 'propaganda' have undergone pejoration (Jatowt &amp; Duh 2014).</skos:example>
    <skos:altLabel xml:lang="en">Degeneration</skos:altLabel>
    <dcterms:bibliographicCitation>Jatowt &amp; Duh (2014). Framework for analyzing semantic change. JCDL. Bloomfield (1933) nine classes.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasSentimentDirection">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has sentiment direction</rdfs:label>
    <rdfs:comment>Direction of sentiment change: pejoration, amelioration, or neutral.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <dcterms:bibliographicCitation>Jatowt &amp; Duh (2014). Sentiment orientation level.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SemanticChangeEvent">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000015"/>
    <rdfs:label>Semantic Change Event</rdfs:label>
    <rdfs:comment>A process by which the meaning of a term changes over time. Top-level class for all semantic change phenomena.</rdfs:comment>
    <skos:definition>A temporal process wherein a linguistic term undergoes modification in its semantic content, usage patterns, or conceptual associations within a discourse community.</skos:definition>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021). Survey of Computational Approaches to Lexical Semantic Change Detection. Language Science Press.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#affectsConcept">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>affects concept</rdfs:label>
    <rdfs:comment>Links a semantic change event to the term/concept undergoing change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition>Relates a semantic change event to the linguistic concept or term that is the subject of the change.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#ExtensionalDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Extensional Drift</rdfs:label>
    <rdfs:comment>Changes in concept instances (things the concept extends to).</rdfs:comment>
    <skos:definition>Semantic change affecting the extensional definition of a concept via modifications to its instances, measured via Jaccard similarity of instance sets.</skos:definition>
    <dcterms:bibliographicCitation>Wang et al. (2009, 2011); Stavropoulos et al. (2019). SemaDrift.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasURIStability">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has URI stability</rdfs:label>
    <rdfs:comment>Measure of identifier consistency (0.0-1.0); 1.0 = no URI changes.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <dcterms:bibliographicCitation>Capobianco et al. (2020). URI stability as quality metric.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasWorkflowStructure">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has workflow structure</rdfs:label>
    <rdfs:comment>Links semantic change detection to process design (prospective provenance).</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Plan"/>
    <dcterms:bibliographicCitation>Missier et al. (2013). D-PROV workflow structure.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#usesExternalKnowledgeBase">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>uses external knowledge base</rdfs:label>
    <rdfs:comment>External knowledge source consulted for semantic correspondence.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <dcterms:bibliographicCitation>Maree &amp; Belkhatir (2015). Multi-source aggregation.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#SignatureBasedMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <rdfs:label>Signature-Based Method</rdfs:label>
    <rdfs:comment>Constructs concept signatures from usage patterns in documents, tracks signature evolution.</rdfs:comment>
    <dcterms:bibliographicCitation>Gulla et al. (2010). Usage-based drift detection.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#CollectiveDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#ExtrinsicDrift"/>
    <rdfs:label>Collective Drift</rdfs:label>
    <rdfs:comment>Entire ontology drifts consistently together; domain evolved but ontology remains internally coherent.</rdfs:comment>
    <skos:definition>A pattern of semantic drift wherein all concepts in an ontology exhibit extrinsic drift but minimal intrinsic drift, indicating domain-wide evolution with preserved internal consistency.</skos:definition>
    <dcterms:bibliographicCitation>Gulla et al. (2010). Concept exposed to extrinsic but not intrinsic drift.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#LexicalObsolescence">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#Decline"/>
    <rdfs:label>Lexical Obsolescence</rdfs:label>
    <rdfs:comment>Word slides into obscurity, becomes archaic.</rdfs:comment>
    <skos:definition>The process by which a lexical item falls out of common usage and becomes obsolete or archaic.</skos:definition>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'obsolete words slide into obscurity'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasExecutionTrace">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has execution trace</rdfs:label>
    <rdfs:comment>Retrospective provenance: WHAT was actually detected.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/ns/prov#Activity"/>
    <dcterms:bibliographicCitation>Missier et al. (2013). D-PROV retrospective provenance.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#ConceptualBridge">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Conceptual Bridge</rdfs:label>
    <rdfs:comment>Work or publication that mediates between different disciplinary meanings, facilitating semantic transfer.</rdfs:comment>
    <skos:definition>A scholarly contribution that explicitly connects or transfers meaning between different disciplinary contexts, enabling cross-domain semantic evolution.</skos:definition>
    <skos:example>Anscombe's 'Intention' (1957): bridges legal concept of agency to philosophical analysis of action.</skos:example>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasExternalCause">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:subPropertyOf rdf:resource="http://ontextract.org/sco#hasCause"/>
    <rdfs:label>has external cause</rdfs:label>
    <rdfs:comment>External driver: cultural, societal, or technological change.</rdfs:comment>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021): 'external factors such as cultural, societal and technological changes'.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#DetectionMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>Detection Method</rdfs:label>
    <rdfs:comment>A computational or analytical approach for detecting semantic change.</rdfs:comment>
    <skos:definition>A systematic methodology for identifying and measuring semantic change phenomena.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#AlignmentBasedMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <rdfs:label>Alignment-Based Method</rdfs:label>
    <rdfs:comment>Aligns embeddings across time periods (e.g., Orthogonal Procrustes). May introduce artifacts.</rdfs:comment>
    <dcterms:bibliographicCitation>Hamilton et al. (2016); Dubossarsky et al. (2019) show alignment introduces noise.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasStartYear">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label>has start year</rdfs:label>
    <rdfs:comment>Calendar year when semantic change began (for query convenience).</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
    <skos:definition>The calendar year marking the beginning of a semantic change event, cached from temporal interval for efficient querying.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#IntensionalDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Intensional Drift</rdfs:label>
    <rdfs:comment>Changes in concept properties (characteristics implied by the concept).</rdfs:comment>
    <skos:definition>Semantic change affecting the intensional definition of a concept via modifications to its properties, measured via Jaccard similarity of property sets.</skos:definition>
    <dcterms:bibliographicCitation>Wang et al. (2009, 2011); Stavropoulos et al. (2019). SemaDrift.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#HybridMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <rdfs:label>Hybrid Method</rdfs:label>
    <rdfs:comment>Combines text similarity, structural similarity, and other approaches.</rdfs:comment>
    <dcterms:bibliographicCitation>Stavropoulos et al. (2019). SemaDrift hybrid approach.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#TemporalReferencingMethod">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#DetectionMethod"/>
    <rdfs:label>Temporal Referencing Method</rdfs:label>
    <rdfs:comment>Avoids alignment by using single vector space with temporal tags (word_1910, word_1920, etc.).</rdfs:comment>
    <dcterms:bibliographicCitation>Dubossarsky et al. (2019). Time-Out. Superior to alignment-based methods.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#LexicalReplacement">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:label>Lexical Replacement</rdfs:label>
    <rdfs:comment>One word replaces another for expressing the same concept.</rdfs:comment>
    <skos:definition>A semantic change process wherein one lexical item supplants another in expressing a particular meaning or concept.</skos:definition>
    <dcterms:bibliographicCitation>Tahmasebi et al. (2021). Lexical replacement patterns.</dcterms:bibliographicCitation>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#hasFromMeaning">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:label>has from meaning</rdfs:label>
    <rdfs:comment>The original or source meaning before the semantic change.</rdfs:comment>
    <rdfs:domain rdf:resource="http://ontextract.org/sco#SemanticChangeEvent"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:definition>Indicates the semantic content or meaning that served as the starting point for a semantic change event.</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="http://ontextract.org/sco#IntrinsicDrift">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="http://ontextract.org/sco#SemanticDrift"/>
    <rdfs:label>Intrinsic Drift</rdfs:label>
    <rdfs:comment>Concept's semantic value changes relative to OTHER concepts in the ontology.</rdfs:comment>
    <skos:definition>Semantic drift wherein a concept's meaning shifts with respect to its relationships with other concepts within the same ontology, typically reflected in changed relationships.</skos:definition>
    <dcterms:bibliographicCitation>Gulla et al. (2010). Semantic Drift in Ontologies. WEBIST.</dcterms:bibliographicCitation>
  </rdf:Description>
</rdf:RDF>
